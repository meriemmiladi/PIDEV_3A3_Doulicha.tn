<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="497.0" prefWidth="857.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="codingbeasts.doulicha.view.AffichageUserController">
   <children>
      <Pane layoutY="-1.0" prefHeight="504.0" prefWidth="257.0" style="-fx-background-color: #A6D4DC;">
         <children>
            <ImageView fitHeight="116.0" fitWidth="123.0" layoutX="63.0" layoutY="52.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/doulicha.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutX="257.0" layoutY="45.0" prefHeight="125.0" prefWidth="600.0" style="-fx-background-color: #A6D4DC;" />
      <TableView fx:id="txttable" layoutX="257.0" layoutY="216.0" prefHeight="288.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="txtnom" prefWidth="75.0" text="Nom" />
          <TableColumn fx:id="txtprenom" prefWidth="75.0" text="Prenom" />
            <TableColumn fx:id="txtemail" prefWidth="189.0" text="Email" />
            <TableColumn fx:id="txtpassword" prefWidth="107.0" text="Mot de Passe" />
            <TableColumn fx:id="txtrole" prefWidth="67.0" text="Role" />
        </columns>
      </TableView>
      <Button contentDisplay="GRAPHIC_ONLY" layoutX="303.0" layoutY="178.0" mnemonicParsing="false" onAction="#deleteuser" text="Button">
         <graphic>
            <FontAwesomeIconView fill="#9b0b31" glyphName="TRASH" size="22" />
         </graphic>
      </Button>
      <Button contentDisplay="GRAPHIC_ONLY" layoutX="352.0" layoutY="179.0" mnemonicParsing="false" onAction="#refreshuser" text="Button">
         <graphic>
            <FontAwesomeIconView fill="#a6d4dc" glyphName="REFRESH" size="22" />
         </graphic>
      </Button>
   </children>
</AnchorPane>
