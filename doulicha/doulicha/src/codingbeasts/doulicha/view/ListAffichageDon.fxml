<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1230.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="codingbeasts.doulicha.controller.ListAffichageDonController">
